// CSS USED

* 
  font-family 'Source Sans Pro'
  font-stretch normal
  font-weight 300

.block
  padding-top 17px
  padding-bottom 17px
  padding-left 17px
  padding-right 17px


.dropdown-toggle::after 
    // display none

.Header-font
  font-size 15px

.Header-logo
  height 85px

.Header-button
  font-size 140px

.line-separator
  height 1px
  background transparent
  border-bottom 1px solid #55bfe0

.Header-title
  font-family 'Futura'
  color #55bfe0
  font-size 36px
  line-height 1.1
  text-align left
  margin-top 40px
  font-weight bold

.Header-subheader
  font-family 'Futura'
  color #434a54
  font-size 18px
  text-align center
  line-height 1.5


.Header-btn-dropdown
  font-family 'Source Sans Pro'
  font-size 15px
  border 0px

div.Header-btn-dropdown-item button 
  font-family 'Source Sans Pro'
  font-size 15px



// OLD CSS 

h1
  font-size 2em

.color-silver
  background-color #DDDDDD

.color-font-aqua
  color #7FDBFF

.color-aqua
  background-color #7FDBFF

.color-blue
  background-color #0074D9

.color-fuchsia
  background-color #F012BE

.font-warning
  color red

.generic-center
  margin auto
  padding 2rem
  border 2px solid #000
  text-align center

.dummy-height 
  min-height 500px

.border
  border 2px solid #000

.routes
  text-align left
  border 2px solid #000

.buttons
  border 2px solid #000
  padding 20px


.borderless 
  border none

  // App.css
.App 
  text-align center


.App-logo 
  // animation App-logo-spin infinite 20s linear
  height 80px




.App-header 
  height 120px
  padding 20px

.App-intro 
  font-size large

.login-transition
  text-align center
  padding-top 50vh

.debugger
  text-align left

@keyframes App-logo-spin 
  from { transform: rotate(0deg) }
  to { transform: rotate(360deg) }



